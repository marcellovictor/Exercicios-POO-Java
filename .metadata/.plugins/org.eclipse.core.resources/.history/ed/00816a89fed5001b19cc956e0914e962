package entidades;

import java.util.ArrayList;
import java.util.List;

public class Casa {

	//ATRIBUTOS
	
	private String cor;
	private List<Porta> portas = new ArrayList<>();
	
	//CONSTRUTORES
	
	public Casa() {
		this.cor = "Branco";
	}
	
	//GETTERS E SETTERS
	
	public String getCor() {
		return cor;
	}
	
	public void setCor(String cor) {
		this.cor = cor;
	}
	
	public List<Porta> getPortas() {
		return portas;
	}
	
	//METODOS
	
	
}
