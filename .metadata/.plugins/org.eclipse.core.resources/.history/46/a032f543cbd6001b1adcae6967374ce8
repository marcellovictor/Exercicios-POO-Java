package entidades;

public class Carta {

	//ATRIBUTOS
	
	private String naipe;
	private String valor;
	
	//CONSTRUTOR
	
	public Carta(String naipe, String valor) {
		this.naipe = naipe;
		this.valor = valor;
	}

	//GETTERS
	
	public String getNaipe() {
		return naipe;
	}

	public String getValor() {
		return valor;
	}
	
	//METODOS
	
	@Override
	public String toString() {
		return valor + " de " + naipe;
	}
	
	
}
